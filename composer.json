{
    "name": "puwnz/google-maps-lib",
    "description": "Library to communicate with Google API",
    "type": "library",
    "keywords": [ "google", "map" ],
    "authors": [
        {
            "name": "Guillaume D.",
            "email": "guigz-style@live.fr"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-intl": "*",
        "ext-json": "*",
        "monolog/monolog": "^2.0",
        "symfony/cache": "^4.0|^5.0",
        "symfony/http-client": "^4.0|^5.0",
        "symfony/validator": "^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^2.16 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "Puwnz\\GoogleMapsLib\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Puwnz\\GoogleMapsLib\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests" : [
            "php vendor/bin/phpunit --coverage-text"
        ]
    },
    "license": "MIT",
    "minimum-stability": "stable"
}
